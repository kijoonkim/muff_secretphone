<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_downloadFragment"
            app:destination="@id/downloadFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_graderFragment"
            app:destination="@id/graderFragment" />
    </fragment>
    <fragment
        android:id="@+id/downloadFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.download.DownloadFragment"
        android:label="fragment_download"
        tools:layout="@layout/fragment_download" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home"
        >
        <argument
            android:name="selecDate"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="selecAmPm"
            app:argType="string"
            android:defaultValue=""/>
    </fragment>
    <fragment
        android:id="@+id/graderFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.grader.GraderFragment"
        android:label="fragment_grader"
        tools:layout="@layout/fragment_grader" >
        <argument
            android:name="selecDate"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="selecAmPm"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="txtId"
            app:argType="string"
            android:defaultValue=""/>
        <action
            android:id="@+id/action_graderFragment_to_videoFragment"
            app:destination="@id/videoFragment" />
    </fragment>
    <fragment
        android:id="@+id/videoFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.video.VideoFragment"
        android:label="fragment_video"
        tools:layout="@layout/fragment_video" >
        <argument
            android:name="videoModel"
            app:argType="string" />
        <argument
            android:name="isEnglish"
            app:argType="boolean" />
        <action
            android:id="@+id/action_videoFragment_to_videoResultFragment"
            app:destination="@id/videoResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/videoResultFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.videoresult.VideoResultFragment"
        android:label="fragment_video_result"
        tools:layout="@layout/fragment_video_result" >
        <argument
            android:name="videoModel"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/uploadFragment"
        android:name="com.muffinmanz.muff_secretphone.ui.upload.UploadFragment"
        android:label="fragment_upload"
        tools:layout="@layout/fragment_upload">
        <argument
            android:name="selecDate"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="selecAmPm"
            app:argType="string"
            android:defaultValue=""/>
    </fragment>
</navigation>